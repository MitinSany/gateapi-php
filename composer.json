{
    "name": "gateio/GateApi",
    "version": "1.2.0",
    "description": "APIv4 futures provides all sorts of futures trading operations. There are public APIs to retrieve the real-time market statistics, and private APIs which needs authentication to trade on user's behalf.",
    "keywords": [
        "gate",
        "openapitools",
        "openapi-generator",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://gate.io/",
    "license": "proprietary",
    "authors": [
        {
            "name": "gateio",
            "homepage": "https://gate.io/"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "GateApi\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "GateApi\\" : "test/" }
    }
}
